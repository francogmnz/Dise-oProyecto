<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.Admin" table="admin">

        <id name="OID" type="string">
            <column name="OIDAdmin"/>
            <generator class="uuid2"/>
        </id>

        <property name="legajoAdmin" type="int">
            <column name="legajo_admin"/>
        </property>

        <property name="nombreAdmin" type="string">
            <column name="nombre_admin"/>
        </property>



        <property name="fechaHoraBajaAdmin" column="fecha_hora_baja_admin" type="timestamp"/>
        
        <many-to-one name="usuario" class="entidades.Usuario" column="OIDUsuario" not-null="false"/>



    </class>

</hibernate-mapping>
