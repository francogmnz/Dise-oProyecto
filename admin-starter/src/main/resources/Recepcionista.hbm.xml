<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.Recepcionista" table="recepcionista">

        <id name="OID" type="string">
            <column name="OIDRecepcionista"/>
            <generator class="uuid2"/>
        </id>

        <property name="legajoRecepcionista" type="int">
            <column name="legajo_recepcionista"/>
        </property>

        <property name="nombreRecepcionista" type="string">
            <column name="nombre_recepcionista"/>
        </property>



        <property name="fechaHoraBajaRecepcionista" column="fecha_hora_baja_recepcionista" type="timestamp"/>
        
        <many-to-one name="usuario" class="entidades.Usuario" column="OIDUsuario" not-null="false"/>



    </class>

</hibernate-mapping>

